{
  "name": "opendocx-node",
  "version": "0.9.2-beta.0",
  "description": "OpenDocx document assembly for Node.js",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha && dotnet test",
    "build": "genversion src/version.js && dotnet build",
    "bump": "npm version --no-git-tag-version prepatch --preid=beta",
    "preversion-disabled": "npm test",
    "version": "npm run build"
  },
  "comment": "npm version (above) gets one of these: patch, minor, major, prepatch, preminor, premajor, prerelease",
  "keywords": [
    "OpenDocx",
    "document",
    "assembly",
    "Word",
    "DOCX",
    "generate",
    "template"
  ],
  "homepage": "https://github.com/opendocx/opendocx",
  "repository": "github:opendocx/opendocx",
  "bugs": {
    "url": "https://github.com/opendocx/opendocx/issues",
    "email": "lowell@opendocx.com"
  },
  "author": {
    "name": "Lowell Stewart",
    "email": "lowell@opendocx.com"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "edge-js": "^11.3.1",
    "path": "^0.12.7",
    "semver": "^6.1.1",
    "util": "^0.11.1",
    "yatte": "^0.9.9-beta.7"
  },
  "devDependencies": {
    "date-fns": "^1.30.1",
    "genversion": "^2.1.1",
    "mocha": "^5.2.0"
  }
}
