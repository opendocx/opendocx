{
  "name": "opendocx-node",
  "version": "1.6.1",
  "description": "OpenDocx document assembly for Node.js",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha && dotnet test",
    "build": "genversion src/version.js && dotnet build",
    "bump": "npm version --no-git-tag-version patch",
    "preversion-disabled": "npm test",
    "version": "npm run build"
  },
  "comment": "npm version (above) gets one of these: patch, minor, major, prepatch, preminor, premajor, prerelease",
  "keywords": [
    "OpenDocx",
    "document",
    "assembly",
    "Word",
    "DOCX",
    "generate",
    "template"
  ],
  "homepage": "https://github.com/opendocx/opendocx",
  "repository": "github:opendocx/opendocx",
  "bugs": {
    "url": "https://github.com/opendocx/opendocx/issues",
    "email": "lowell@opendocx.com"
  },
  "author": {
    "name": "Lowell Stewart",
    "email": "lowell@opendocx.com"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "date-fns": "^2.15.0",
    "edge-js": "^19.3.0",
    "path": "^0.12.7",
    "semver": "^6.3.0",
    "tiny-async-pool": "^1.2.0",
    "util": "^0.11.1",
    "uuid": "^3.3.3",
    "yatte": "^1.6.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "genversion": "^2.2.1",
    "mocha": "^10.2.0",
    "standard": "^14.3.4"
  }
}
